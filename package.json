{
  "name": "rename-css-selectors",
  "version": "3.2.8",
  "description": "Rename css classes and id's in files",
  "main": "dest",
  "scripts": {
    "test": "jest --coverage",
    "build": "tsc && babel dest -d dest",
    "lint": "eslint lib test",
    "fix": "npm run lint -- --fix",
    "prepublish": "npm run build",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "rcs": {
    "exclude": [
      "js",
      "no-js"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit",
    "files": [
      "test/**/*.js",
      "!test/files/**/*.js"
    ]
  },
  "keywords": [
    "css",
    "minify",
    "rename",
    "selectors",
    "shrink",
    "classes",
    "compress",
    "styles",
    "munch"
  ],
  "repository": {
    "url": "https://www.github.com/JPeer264/rename-css-selectors",
    "type": "git"
  },
  "author": "Jan Peer St√∂cklmair",
  "license": "MIT",
  "dependencies": {
    "async": "^2.1.4",
    "fs-extra": "^2.0.0",
    "glob": "^7.1.1",
    "json-extra": "^0.5.0",
    "lodash.merge": "^4.6.2",
    "universalify": "^0.1.2"
  },
  "peerDependencies": {
    "rcs-core": "^3.*"
  },
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "coveralls": "^2.11.15",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "html-minifier": "^3.5.20",
    "jest": "^25.1.0",
    "nyc": "^12.0.2",
    "rcs-core": "^3.0.1",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5"
  }
}
