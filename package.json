{
  "name": "rename-css-selectors",
  "version": "3.2.5",
  "description": "Rename css classes and id's in files",
  "main": "./index.js",
  "scripts": {
    "test": "nyc ava -s",
    "lint": "eslint lib index.js test",
    "fix": "npm run lint -- --fix",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "rcs": {
    "exclude": [
      "js",
      "no-js"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit",
    "files": [
      "test/**/*.js",
      "!test/files/**/*.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "css",
    "minify",
    "rename",
    "selectors",
    "shrink",
    "classes",
    "compress",
    "styles",
    "munch"
  ],
  "repository": {
    "url": "https://www.github.com/JPeer264/rename-css-selectors",
    "type": "git"
  },
  "author": "Jan Peer St√∂cklmair",
  "license": "MIT",
  "dependencies": {
    "async": "^2.1.4",
    "fs-extra": "^2.0.0",
    "glob": "^7.1.1",
    "json-extra": "^0.5.0",
    "lodash.merge": "^4.6.1",
    "minimatch": "^3.0.4",
    "rcs-core": "^3.0.0-alpha.1",
    "universalify": "^0.1.2"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "coveralls": "^2.11.15",
    "eslint": "^4.2.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.7.0",
    "html-minifier": "^3.5.20",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.3",
    "nyc": "^12.0.2"
  }
}
